"use client";
import { useRouter } from "next/navigation";

export default function Home() {
  const router = useRouter();
  
  return (
    <>
      <div className="flex flex-col justify-center align-items">
        <button onClick={() => router.push("/dsa")} className="flex justify-content">DSA</button>
        <button onClick={() => router.push("/core")} className="flex justify-center">Core</button>
      </div>
    </>
  );
}
